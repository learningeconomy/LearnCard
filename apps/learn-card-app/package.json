    "scripts": {
        "dev": "docker compose -f compose-local.yaml up --build",
        "dev-no-build": "docker compose -f compose-local.yaml up",
        "dev-build-brain": "docker compose -f compose-local.yaml up --build brain",
        "start": "vite --host",
        "start-p-4000": "vite --port=4000",
        "docker-start": "LCN_URL=http://localhost:4000/trpc LCN_API_URL=http://localhost:4000/api CLOUD_URL=http://localhost:4100/trpc API_URL=http://localhost:5100/trpc vite --host",
        "serve": "vite",
        "build": "NODE_OPTIONS=\"--max-old-space-size=16608\"  vite build",
        "docker-build": "LCN_URL=http://localhost:4000/trpc CLOUD_URL=http://localhost:4100/trpc API_URL=http://localhost:5100/trpc NODE_OPTIONS=\"--max-old-space-size=4608\" vite build -d",
        "test-playwright": "playwright test",
        "test-ui": "playwright test --ui",
        "test": "echo \"Actually we don't want to run playwright tests in CI... Use test-playwright or test-ui if you're not CI\"",
        "test-debug": "PLAYWRIGHT_SLOW_MO=1000 npx playwright test --headed --debug",
        "test-report": "playwright show-report",
        "write-test": "playwright codegen --ignore-https-errors --load-storage=tests/states/demoState.json http://localhost:3000",
        "write-test-firefox": "playwright codegen --browser=firefox --ignore-https-errors --load-storage=demoState.json http://localhost:3000",
        "write-test-safari": "playwright codegen --browser=webkit --ignore-https-errors --load-storage=demoState.json http://localhost:3000",
        "start-android": "adb reverse tcp:3000 tcp:3000 && ionic cap run android -l --port=3000",
        "ionic:build": "vite build",
        "ionic:serve": "vite",
        "appflow:install": "npx pnpm install --loglevel=warn --ignore-scripts --no-frozen-lockfile && npx nx build learn-card-app",
        "preview": "vite preview --host --port=3000",
        "neo4j-browser": "open-cli http://localhost:7474/browser?preselectAuthMethod=NO_AUTH"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
