{
  "name": "learn-card-browser-extension",
  "root": "apps/learn-card-browser-extension",
  "sourceRoot": "apps/learn-card-browser-extension/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "pnpm --filter learn-card-browser-extension build"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter learn-card-browser-extension dev"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/coverage"],
      "options": {
        "command": "pnpm --filter learn-card-browser-extension test"
      }
    }
  },
  "tags": []
}
