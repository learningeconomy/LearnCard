{
  "manifest_version": 3,
  "name": "LearnCard",
  "description": "Save digital credentials to your LearnCard in one click.",
  "version": "1.0.0",
  "action": {
    "default_title": "LearnCard",
    "default_popup": "src/popup/index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/main.ts-Cq63oivV.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/main.ts-Cq63oivV.js",
        "assets/main.ts-Cq63oivV.js.map"
      ],
      "use_dynamic_url": false
    }
  ]
}
