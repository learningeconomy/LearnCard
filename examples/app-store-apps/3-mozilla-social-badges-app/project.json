  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "examples/app-store-apps/3-mozilla-social-badges-app/src",
  "targets": {
    "build": {
      "executor": "@nx/run-commands:run-commands",
      "options": {
        "command": "pnpm run build",
        "cwd": "examples/app-store-apps/3-mozilla-social-badges-app"
      }
