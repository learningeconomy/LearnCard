{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "sss-key-manager",
  "sourceRoot": "packages/sss-key-manager/src",
  "projectType": "library",
  "root": "packages/sss-key-manager",
  "tags": [],
  "implicitDependencies": ["nx-run-command"],
  "namedInputs": {
    "source": ["{projectRoot}/src/**/*"],
    "scripts": ["{projectRoot}/scripts/**/*"],
    "tests": ["{projectRoot}/tests/**/*"],
    "dist": ["{projectRoot}/dist/**/*"]
  },
  "targets": {
    "build": {
      "executor": "nx:run-script",
      "inputs": ["source", "scripts"],
      "options": {
        "script": "build"
      }
    },
    "dev": {
      "executor": "nx:run-script",
      "options": {
        "script": "dev"
      }
    },
    "test": {
      "executor": "nx:run-script",
      "inputs": ["source", "tests"],
      "options": {
        "script": "test"
      }
    },
    "typecheck": {
      "executor": "nx:run-script",
      "options": {
        "script": "typecheck"
      }
    }
  }
}
