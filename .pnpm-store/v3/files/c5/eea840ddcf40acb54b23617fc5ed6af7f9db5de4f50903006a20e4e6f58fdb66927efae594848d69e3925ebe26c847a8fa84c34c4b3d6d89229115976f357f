.apg-line-end{font-weight: bold; color: #000000;}
.apg-left-table{font-family:monospace;}
.apg-left-table,
.apg-left-table th,
.apg-left-table td{text-align:left;border:1px solid black;border-collapse:collapse;}
.apg-left-table caption{font-size:125%;font-weight:bold;text-align:left;}
.apg-right-table{font-family:monospace;}
.apg-right-table,
.apg-right-table th,
.apg-right-table td{text-align:right;border:1px solid black;border-collapse:collapse;}
.apg-right-table caption{font-size:125%;font-weight:bold;text-align:left;}
.apg-last-left-table{font-family:monospace;}
.apg-last-left-table,
.apg-last-left-table th,
.apg-last-left-table td{text-align:right;border:1px solid black;border-collapse:collapse;}
.apg-last-left-table th:last-child{text-align:left;}
.apg-last-left-table td:last-child{text-align:left;}
.apg-last-left-table caption{font-size:125%;font-weight:bold;text-align:left;}
.apg-last2-left-table{font-family:monospace;}
.apg-last2-left-table,
