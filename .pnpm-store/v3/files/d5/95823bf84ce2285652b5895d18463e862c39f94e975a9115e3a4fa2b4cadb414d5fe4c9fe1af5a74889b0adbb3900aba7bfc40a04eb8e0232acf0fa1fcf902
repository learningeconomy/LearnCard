{"version":3,"file":"b64u-lite.mjs","sources":["../index.mjs"],"sourcesContent":["import { atob, btoa, toBase64, fromBase64, toBuffer as toBuffer64 } from 'b64-lite'\n\nexport function toBinaryString(b64u) {\n  return atob(b64u\n    .replace(/-/g, '+')\n    .replace(/_/g, '/'))\n}\n\nexport function fromBinaryString(binaryString, padding) {\n  return btoa(binaryString)\n    .replace(/\\+/g, '-')\n    .replace(/\\//g, '_')\n    .replace(/=/g, padding ? '=' : '')\n}\n\nexport function toBase64Url(input, padding) {\n  return toBase64(input)\n    .replace(/\\+/g, '-')\n    .replace(/\\//g, '_')\n    .replace(/=/g, padding ? '=' : '')\n}\n\nexport function fromBase64Url(b64u) {\n  return fromBase64(b64u\n    .replace(/-/g, '+')\n    .replace(/_/g, '/'))\n}\n\nexport function toBuffer(b64u) {\n  return toBuffer64(b64u\n    .replace(/-/g, '+')\n    .replace(/_/g, '/'))\n}\n"],"names":["toBinaryString","b64u","atob","replace","fromBinaryString","binaryString","padding","btoa","toBase64Url","input","toBase64","fromBase64Url","fromBase64","toBuffer","toBuffer64"],"mappings":"sFAEO,SAASA,EAAeC,UACtBC,EAAKD,EACTE,QAAQ,KAAM,KACdA,QAAQ,KAAM,MAGZ,SAASC,EAAiBC,EAAcC,UACtCC,EAAKF,GACTF,QAAQ,MAAO,KACfA,QAAQ,MAAO,KACfA,QAAQ,KAAMG,EAAU,IAAM,IAG5B,SAASE,EAAYC,EAAOH,UAC1BI,EAASD,GACbN,QAAQ,MAAO,KACfA,QAAQ,MAAO,KACfA,QAAQ,KAAMG,EAAU,IAAM,IAG5B,SAASK,EAAcV,UACrBW,EAAWX,EACfE,QAAQ,KAAM,KACdA,QAAQ,KAAM,MAGZ,SAASU,EAASZ,UAChBa,EAAWb,EACfE,QAAQ,KAAM,KACdA,QAAQ,KAAM"}