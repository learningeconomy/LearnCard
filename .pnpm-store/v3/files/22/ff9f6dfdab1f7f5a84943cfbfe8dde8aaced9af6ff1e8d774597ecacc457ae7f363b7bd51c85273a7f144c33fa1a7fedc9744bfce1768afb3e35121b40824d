 */var ReactIs$1=reactIs$1.exports;var assign=objectAssign;var ReactPropTypesSecret$1=ReactPropTypesSecret_1;var has=has$2;var checkPropTypes=checkPropTypes_1;var printWarning=function printWarning(){};if(process.env.NODE_ENV!=='production'){printWarning=function printWarning(text){var message='Warning: '+text;if(typeof console!=='undefined'){console.error(message);}try{// --- Welcome to debugging React ---
