import _objectWithoutProperties from"@babel/runtime/helpers/objectWithoutProperties";import _extends from"@babel/runtime/helpers/extends";import _toConsumableArray from"@babel/runtime/helpers/toConsumableArray";import*as React from"react";import makeClass from"clsx";import{arrayify}from"./arrayify";export function styled(element,options){var defaultDescription="This component accepts all HTML attributes for a \"".concat(element,"\" element.");var _ref=typeof options==="string"?{"class":options}:options,className=_ref["class"],description=_ref.description,name=_ref.name,slot=_ref.slot;var elements=arrayify(element);var ElementsReduced=_toConsumableArray(elements).reduce(function(Accumulator,CurrentValue){return function(asProps){return React.createElement(CurrentValue,_extends({},asProps,{as:Accumulator}))}});var Wrapped=React.forwardRef(function(props,ref){var as=props.as,rest=_objectWithoutProperties(props,["as"]);var Component=as||ElementsReduced;return React.createElement(Component,_extends({ref:ref},rest,{className:makeClass(className,props.className)}))});Wrapped._SLOT_=slot||Symbol(elements[0]);Wrapped.displayName=name;Wrapped.__docgenInfo={description:description?"".concat(description," ").concat(defaultDescription):defaultDescription};return Wrapped}
