export declare const MouseButton: {
    readonly primary: 0;
    readonly secondary: 1;
    readonly auxiliary: 2;
    readonly back: 3;
    readonly X1: 3;
    readonly forward: 4;
    readonly X2: 4;
};
export declare type MouseButton = keyof typeof MouseButton | number;
export declare function getMouseButton(button: MouseButton): number;
export declare function getMouseButtons(...buttons: Array<MouseButton>): number;
