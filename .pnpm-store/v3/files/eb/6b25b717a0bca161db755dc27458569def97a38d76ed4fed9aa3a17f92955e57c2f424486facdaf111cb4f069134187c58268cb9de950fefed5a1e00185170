import { OnResolveResult, Plugin } from 'esbuild';
interface Options {
    external?: (path: string) => boolean;
    onUnresolved?: (e: Error) => OnResolveResult | undefined | null | void;
}
export declare function NodeResolvePlugin({ external, onUnresolved, }?: Options): Plugin;
export default NodeResolvePlugin;
