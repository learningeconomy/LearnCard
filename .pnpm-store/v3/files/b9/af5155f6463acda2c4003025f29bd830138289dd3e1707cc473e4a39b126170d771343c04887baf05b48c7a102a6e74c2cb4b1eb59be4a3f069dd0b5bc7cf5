export var WINDOW_RESIZE_DELAY=150;export function debounce(func){var wait=arguments.length>1&&arguments[1]!==undefined?arguments[1]:WINDOW_RESIZE_DELAY;var immediate=arguments.length>2&&arguments[2]!==undefined?arguments[2]:false;var timeout;return function x(){for(var _len=arguments.length,args=new Array(_len),_key=0;_key<_len;_key++){args[_key]=arguments[_key]}var later=function later(){timeout=null;if(!immediate)func.apply(void 0,args)};var callNow=immediate&&!timeout;if(timeout){clearTimeout(timeout)}timeout=setTimeout(later,wait);if(callNow)func.apply(void 0,args)}}
