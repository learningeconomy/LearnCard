import { EventType, PointerCoords } from '../event';
import { Config } from '../setup';
import { MouseButton } from '../utils';
export declare function firePointerEvent(config: Config, target: Element, type: EventType, { pointerType, button, coords, pointerId, isPrimary, clickCount, }: {
    pointerType?: 'mouse' | 'pen' | 'touch';
    button?: MouseButton;
    coords?: PointerCoords;
    pointerId?: number;
    isPrimary?: boolean;
    clickCount?: number;
}): boolean;
