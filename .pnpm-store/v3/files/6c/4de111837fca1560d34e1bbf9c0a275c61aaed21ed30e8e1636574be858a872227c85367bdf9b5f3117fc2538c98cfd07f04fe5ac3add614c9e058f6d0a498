import { IdentifierSpec } from "./types";
export interface ChainIdParams {
    namespace: string;
    reference: string;
}
export declare class ChainId {
    static spec: IdentifierSpec;
    static parse(id: string): ChainIdParams;
    static format(params: ChainIdParams): string;
    namespace: string;
    reference: string;
    constructor(params: ChainIdParams | string);
    toString(): string;
    toJSON(): ChainIdParams;
}
