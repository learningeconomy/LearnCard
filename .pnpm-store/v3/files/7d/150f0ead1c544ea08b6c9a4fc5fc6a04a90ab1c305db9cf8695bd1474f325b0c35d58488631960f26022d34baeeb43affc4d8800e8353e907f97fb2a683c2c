import{atob as e,btoa as r,toBase64 as c,fromBase64 as n,toBuffer as p}from"b64-lite";function t(r){return e(r.replace(/-/g,"+").replace(/_/g,"/"))}function a(e,c){return r(e).replace(/\+/g,"-").replace(/\//g,"_").replace(/=/g,c?"=":"")}function l(e,r){return c(e).replace(/\+/g,"-").replace(/\//g,"_").replace(/=/g,r?"=":"")}function g(e){return n(e.replace(/-/g,"+").replace(/_/g,"/"))}function u(e){return p(e.replace(/-/g,"+").replace(/_/g,"/"))}export{t as toBinaryString,a as fromBinaryString,l as toBase64Url,g as fromBase64Url,u as toBuffer};
