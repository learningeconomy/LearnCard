git_url("git@github.com:WeLibraryOS/certificates.git")

storage_mode("git")

type(ENV['CODE_SIGNING_TYPE'])

app_identifier([ENV['APP_ID']])
username(ENV['APPLE_ID']) # Your Apple Developer Portal username

team_id(ENV['TEAM_ID'])

git_branch(ENV['MATCH_GIT_BRANCH'])
keychain_password(ENV['MATCH_PASSWORD'])
git_private_key(ENV['MATCH_GIT_DEPLOY_KEY'])

